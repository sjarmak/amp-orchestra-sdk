{
  "name": "@ampsm/workspace",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --target node20",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "lint": "echo 'No linting configured'"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^24.3.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.5"
  }
}
